<h1 id="back-block">
    <a href="/clinician">&lt; back: to patient view</a>
</h1>

<h2 id="notes-header">History of Notes on Patient: Pat Patrick</h2>

<div class="history-table-border">
    <table class="history-table">
        <tr>
            <th>Date</th>
            <th>Time</th>
            <th>Notes</th>
        </tr>
        <!--iterate over all notes and display-->
        {{#each notes}}
            <tr>
                <td>{{formatDate this.datetime "DD/MM/YYYY"}}</td>
                <td>{{formatDate this.datetime "hh:mm A"}}</td>
                <td>{{this.note}}</td>
            </tr>
        {{/each}}
    </table>
</div>